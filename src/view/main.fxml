<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Main">
   <children>
      <TabPane layoutX="14.0" layoutY="45.0" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab text="    Appointments    ">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <HBox alignment="CENTER_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label text="Month" />
                                    <ComboBox fx:id="monthCombo" nodeOrientation="LEFT_TO_RIGHT" onAction="#monthComboAction" />
                                    <Separator orientation="VERTICAL" />
                                    <CheckBox fx:id="weekCheckBox" mnemonicParsing="false" onAction="#onCheckWeekBox" text="Week of" />
                                    <ComboBox fx:id="weekCombo" disable="true" />
                                    <Region HBox.hgrow="ALWAYS" />
                                    <Button minWidth="150.0" mnemonicParsing="false" text="New Appointment" />
                                 </children>
                              </HBox>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </HBox>
                        <TableView layoutY="35.0" AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="45.0">
                          <columns>
                            <TableColumn maxWidth="1200.0" prefWidth="89.0" text="Date" />
                            <TableColumn maxWidth="1000.0" prefWidth="53.0" text="Start" />
                              <TableColumn maxWidth="1000.0" prefWidth="207.0" text="End" />
                              <TableColumn prefWidth="75.0" text="Type" />
                              <TableColumn maxWidth="1600.0" prefWidth="75.0" text="Customer" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox alignment="CENTER_RIGHT" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                           <children>
                              <Button minWidth="70.0" mnemonicParsing="false" text="Edit" />
                              <Button layoutX="738.0" layoutY="10.0" minWidth="70.0" mnemonicParsing="false" text="Delete" />
                           </children>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </HBox>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="       Customers       ">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <children>
                              <Button minWidth="150.0" mnemonicParsing="false" text="New Customer" />
                           </children>
                        </HBox>
                        <TableView layoutY="35.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="45.0">
                          <columns>
                            <TableColumn maxWidth="3000.0" prefWidth="75.0" text="Name" />
                            <TableColumn prefWidth="75.0" text="Address" />
                              <TableColumn maxWidth="1500.0" prefWidth="75.0" text="Phone" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox alignment="CENTER_RIGHT" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                           <children>
                              <Button minWidth="70.0" mnemonicParsing="false" text="Edit" />
                              <Button layoutX="738.0" layoutY="10.0" minWidth="70.0" mnemonicParsing="false" text="Delete" />
                           </children>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </HBox>
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </TabPane>
   </children>
</AnchorPane>
